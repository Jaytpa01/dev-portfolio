{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@content/*": ["src/content/*"],
			"@sections/*": ["src/sections/*"],
			"@assets/*": ["src/assets/*"],
			"@lib/*": ["src/lib/*"]
		},
		"allowJs": true,
		"jsx": "react-jsx",
		"jsxImportSource": "preact"
	}
}
